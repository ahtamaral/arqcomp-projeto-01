		
		# min_idx = i
		add $t4, $t2, $zero
		
		

		
		# for (j = i + 1; j < n; j++) {}
		
		# j = i + 1
		add $t6, $t3, $t2
		addi $t3, $t6, 1

		# printf("%d%d", i, min_idx)
		li $v0, 1
		move $a0, $t2
		syscall
		move $a0, $t3
		syscall
		
		inner_for:
			bge $t3, $t5, exit_inner_for
			
			
			addi $t3, $t3, 1
			j inner_for
		exit_inner_for:

		# --- 